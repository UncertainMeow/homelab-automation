# Homepage Environment Variables
# Copy to .env and fill in your values

# Proxmox API Token (create in Datacenter > Permissions > API Tokens)
# Format: username@realm!tokenid=token-secret
HOMEPAGE_VAR_PROXMOX_TOKEN=your-proxmox-api-token

# PBS API Token
HOMEPAGE_VAR_PBS_TOKEN=your-pbs-api-token

# UniFi API Key (same as used for DNS sync)
HOMEPAGE_VAR_UNIFI_KEY=KdUEfY_59qUUkMjPyZsJtIpNIeYVfjhA

# Technitium DNS Token (get from login API)
HOMEPAGE_VAR_DNS_TOKEN=your-dns-token
