# GitLab Stack Configuration
# Managed by Ansible - DO NOT EDIT MANUALLY
# Host: {{ inventory_hostname }}

GITLAB_VERSION={{ gitlab_version }}
GITLAB_EXTERNAL_URL={{ gitlab_external_url }}
GITLAB_HOSTNAME={{ gitlab_hostname }}
GITLAB_HTTP_PORT={{ gitlab_http_port }}
GITLAB_HTTPS_PORT={{ gitlab_https_port }}
GITLAB_SSH_PORT={{ gitlab_ssh_port }}
GITLAB_TIMEZONE={{ gitlab_timezone }}
{% if gitlab_registry_url %}
REGISTRY_EXTERNAL_URL={{ gitlab_registry_url }}
{% endif %}
{% if gitlab_deploy_runner %}
RUNNER_VERSION={{ gitlab_runner_version }}
{% endif %}
