---
# Encrypted Vault File Template
# Copy to vault.yml and encrypt with: ansible-vault encrypt vault.yml

# ============================================
# Tailscale Authentication
# ============================================
# Generate at: https://login.tailscale.com/admin/settings/keys
# Options:
#   - Reusable: Yes
#   - Ephemeral: No
#   - Preauthorized: Yes
#   - Tags: tag:beacon
#   - Expiry: 90 days
tailscale_auth_key: "tskey-auth-XXXXXXXXXXXXX-YYYYYYYYYYYYYY"

# ============================================
# Email Notifications (Optional)
# ============================================
# smtp_username: "your-email@gmail.com"
# smtp_password: "your-app-password"

# ============================================
# DNS Server (Technitium)
# ============================================
vault_dns_admin_password: "your-secure-dns-admin-password"

# ============================================
# Netbox IPAM
# ============================================
vault_netbox_db_password: "your-secure-postgres-password"
vault_netbox_redis_password: "your-secure-redis-password"
vault_netbox_superuser_password: "your-secure-admin-password"
vault_netbox_secret_key: "your-long-random-django-secret-key-at-least-50-chars"

# Create this after first Netbox login (Admin â†’ API Tokens)
vault_netbox_api_token: ""

# ============================================
# Other Secrets
# ============================================
# Add other sensitive data here as needed
